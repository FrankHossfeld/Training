<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='module0810'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Other module inherits                                      -->
  <inherits name='com.mvp4g.Mvp4gModule'/>

  <inherits name="de.gishmo.gwt.example.module0503.ModulePerson" />
  <inherits name="de.gishmo.gwt.example.module0707.Module0707" />
  
  <inherits name='com.sencha.gxt.ui.GXT' />
  <!-- Triton Theme -->
  <inherits name="com.sencha.gxt.theme.triton.Theme" />

  <!-- GXT Stylesheet -->
  <stylesheet src="reset.css" />
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='de.gishmo.gwt.example.module0810.client.Module0810'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  
  <!-- Deferred binding rules -->
  <replace-with class="de.gishmo.gwt.example.module0810.client.ui.detail.DetailView">
    <when-type-is class="de.gishmo.gwt.example.module0810.client.ui.detail.IDetailView"/>
  </replace-with>  
  <replace-with class="de.gishmo.gwt.example.module0810.client.ui.list.ListView">
    <when-type-is class="de.gishmo.gwt.example.module0810.client.ui.list.IListView"/>
  </replace-with>  
  <replace-with class="de.gishmo.gwt.example.module0810.client.ui.navigation.NavigationView">
    <when-type-is class="de.gishmo.gwt.example.module0810.client.ui.navigation.INavigationView"/>
  </replace-with>  
  <replace-with class="de.gishmo.gwt.example.module0810.client.ui.search.SearchView">
    <when-type-is class="de.gishmo.gwt.example.module0810.client.ui.search.ISearchView"/>
  </replace-with>  
  <replace-with class="de.gishmo.gwt.example.module0810.client.ui.shell.ShellView">
    <when-type-is class="de.gishmo.gwt.example.module0810.client.ui.shell.IShellView"/>
  </replace-with>  
</module>

<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:de.gishmo.gwt.example.module0708.client.widgets.form">

  <g:ScrollPanel>
    <g:FlowPanel ui:field="searchPanel">
      <g:Label ui:field="headline"/>
      <w:TextField ui:field="searchName"/>
      <w:TextField ui:field="searchCity"/>
      <g:FlowPanel ui:field="buttonBar">
        <g:Button ui:field="searchButton"/>
        <g:Button ui:field="resetButton"/>
      </g:FlowPanel>
    </g:FlowPanel>
  </g:ScrollPanel>
</ui:UiBinder>